{

  "redirects": [
    {
      "source": "/",
      "destination": "https://www.essentiallabinc.com",
      "permanent": true
    }
  ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Security-Policy", "value": "default-src 'self' 'unsafe-inline' https: data:" }
        ]
      }
    ]
  }